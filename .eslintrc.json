{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "react-app",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/prefer-default-export": [
            0
        ],
        "no-restricted-syntax": [
            0
        ],
        "no-console": [
            0
        ],
        "no-alert": [
            0
        ],
        "class-methods-use-this": [
            0
        ],
        "react/jsx-filename-extension": [
            0
        ],
        "jsx-a11y/click-events-have-key-events": [
            0
        ],
        "jsx-a11y/no-static-element-interactions": [
            0
        ]
    }
}